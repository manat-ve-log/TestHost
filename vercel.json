{
  "headers": [
    {
      "source": "/assets/*.js",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/old-directory/(.*)",
      "destination": "/new-directory/$1",
      "permanent": true
    }
  ]
}
